<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 节点服务器表 -->
		<object name="expNode" table="t_experiment_node" primary="id"
			unique="" order="id desc">
			<property name="id" type="Long" length="20" initial=""
				field="id" />
			<property name="nodeId" type="Long" length="20" initial=""
				field="node_id" />
			<property name="code" type="String" length="20" field="" />
			<property name="name" type="String" length="20" field="" />
			<property name="ip" type="String" length="50" field="" />
			<property name="port" type="String" length="50" field="" />
			<property name="accountNumber" type="String" length="50"
				field="" />
			<property name="password" type="String" length="50" field="" />
			<property name="emulator" type="String" length="200"
				field="" />
			<property name="domainStoragePoolSrc" type="String"
				length="200" field="" />
			<property name="maxDomains" type="Long" length="20"
				initial="" field="" />
			<property name="maxMemory" type="Long" length="20"
				initial="" field="" />
			<property name="minDomains" type="Long" length="20"
				initial="" field="" />
			<property name="scheduleId" type="Long" length="10"
				initial="" field="schedule_id" />
		</object>

	</model_layer>
</tsoftware>
