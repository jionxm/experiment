<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 节点服务器表 -->
		<object name="DisNode" table="t_distribution_node" primary="id" unique=""
			order="id desc">
			<property name="id" type="Long" length="10" initial="" field="id" />
			<property name="planId" type="Long" length="10" initial="" field="plan_id" />
			<property name="nodeId" type="Long" length="10" initial="" field="node_id" />
			<property name="code" type="String" length="20" field="" />
			<property name="name" type="String" length="20" field="" />	
			<property name="ip" type="String" length="50" field="" />
			<property name="port" type="String" length="50" field="" />	
			<property name="accountNumber" type="String" length="50" field="" />	
			<property name="password" type="String" length="50" field="" />	
			<property name="type" type="String" length="20" field="" />	
			<property name="typeName" type="String" length="20" field="" />	
		</object>
	</model_layer>
</tsoftware>
