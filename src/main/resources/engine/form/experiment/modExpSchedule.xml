<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 课程表 -->
		<object name="expSchedule" table="t_experiment_schedule"
			primary="id" unique="" order="createTime desc">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="name" type="String" length="20" field="name" />
			<property name="egId" type="Long" length="10" field="eg_id" />
			<property name="teacherId" type="Long" length="10" field="teacher_id" />
			<property name="courseId" type="Long" length="10" field="course_id" />
			<property name="status" type="Long" length="1" field="status" />
			<property name="endTime" type="Date" length="" field="end_time" />
			<property name="beginAddress" type="String" length="20" field="begin_address" />
			<property name="endAddress" type="String" length="20" field="end_address" />
			<property name="countDown" type="Long" length="20" field="count_down" />
			<property name="createTime" type="Date" length="" validate=""
				field="create_time" />
			<property name="createBy" type="Long" length="20" validate=""
				field="create_by" />
			<property name="updateTime" type="Date" length="" validate=""
				field="update_time" />
			<property name="updateBy" type="Long" length="20" validate=""
				field="update_by" />
			<property name="updateCount" type="Long" length="11"
				validate="" field="update_count" />

			<property name="teacherName" type="String" length="20"
				field="" />
			<property name="courseName" type="String" length="20"
				field="" />
			<property name="egName" type="String" length="20" field="" />
			<property name="statusName" type="String" length="20" field="" />
			<property name="createByName" type="String" length="50"
				field="" />
			<property name="updateByName" type="String" length="50"
				field="" />
			<property name="demand" type="String" length="500" field="" />
			<property name="target" type="String" length="500" field="" />
			<property name="content" type="String" length="500" field="" />
			<property name="reportDemand" type="String" length="500" field="" />
			<property name="reportTarget" type="String" length="500" field="" />
			<property name="reportContent" type="String" length="500" field="" />
			<property name="fileId" type="Long" length="10" field="" />
			<property name="fileName" type="String" length="50" field="" />
			<property name="scheduleId" type="Long" length="20" field="" />
		</object>
	</model_layer>
</tsoftware>
