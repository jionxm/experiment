<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 资源分配方式表 -->
		<object name="distributionPlan" table="t_distribution_plan"
			primary="id" unique="" order="createTime desc">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="type" type="String" length="10" field="type" />
			<property name="typeName" type="String" length="10" field="" />
			<property name="decision" type="Long" length="10" field="decision" />
			<property name="distribution" type="String" length="200"
				field="distribution" />
			<property name="createTime" type="Date" length="" validate=""
				field="create_time" />
			<property name="createBy" type="Long" length="20" validate=""
				field="create_by" />
			<property name="updateTime" type="Date" length="" validate=""
				field="update_time" />
			<property name="updateBy" type="Long" length="20" validate=""
				field="update_by" />
			<property name="updateCount" type="Long" length="11"
				validate="" field="update_count" />
			<property name="createByName" type="String" length="50" field="" />
			<property name="updateByName" type="String" length="50" field="" />
			
		</object>
	</model_layer>
</tsoftware>
