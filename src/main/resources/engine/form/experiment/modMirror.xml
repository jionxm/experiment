<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 节点服务器表 -->
		<object name="mirror" table="t_mirror" primary="id" unique=""
			order="id desc">
			<property name="id" type="Long" length="20" initial=""
				field="id" />
			<property name="mirrorId" type="Long" length="20" initial=""
				field="" />
			<property name="cpu" type="Long" length="20" initial=""
				field="cpu" />
			<property name="memory" type="Long" length="20" initial=""
				field="memory" />
			<property name="hardDisk" type="Long" length="20" initial=""
				field="hard_disk" />
			<property name="mirror" type="String" length="20"
				field="mirror" />
			<property name="mirrorUsername" type="String" length="20"
				field="mirror_username" />
			<property name="mirrorPassword" type="String" length="20"
				field="mirror_password" />
		</object>
	</model_layer>
</tsoftware>
