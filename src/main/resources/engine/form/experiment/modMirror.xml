<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 节点服务器表 -->
		<object name="mirror" table="t_mirror" primary="id" unique=""
			order="id desc">
			<property name="id" type="Long" length="20" initial=""
				field="id" />
			<property name="mirrorId" type="Long" length="20" initial=""
				field="" />
			<property name="mirror" type="String" length="20"
				field="mirror" />
			<property name="mirrorUsername" type="String" length="20"
				field="mirror_username" />
			<property name="mirrorPassword" type="String" length="20"
				field="mirror_password" />
		</object>
		<object name="mirrorPlan" table="t_mirror_plan" primary="id"
			unique="" order="id desc">
			<property name="id" type="Long" length="20" initial=""
				field="id" />
			<property name="mirrorId" type="Long" length="20"
				field="mirror_id" />
			<property name="planId" type="Long" length="20"
				field="plan_id" />
			<property name="decision" type="String" length="10"
				initial="" field="" />
			<property name="type" type="String" length="10" initial=""
				field="" />
			<property name="distribution" type="String" length="10"
				initial="" field="" />
		</object>
	</model_layer>
</tsoftware>
