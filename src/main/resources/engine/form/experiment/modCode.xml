<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 源代码表 -->
		<object name="code" table="t_code" primary="id" unique="" order="createTime desc">
			<property name="id" type="Long" length="10" initial="" field="id" />
			<property name="gitPath" type="String" length="100" initial="" field="git_path" />
			<property name="decision" type="String" length="200" field="decision" />
			<property name="type" type="String" length="10" field="type" />
			<property name="typeName" type="String" length="10" field="" />
			<property name="fileId" type="Long" length="10" field="file_id" />
			<property name="flag" type="String" length="1" field="flag" />
			<property name="flagName" type="String" length="1" field="" />
			<property name="fileName" type="String" length="20" field="" />
			<property name="presetName" type="String" length="10" field="" />
			<property name="preset" type="String" length="1" field="preset" />
			<property name="createTime" type="Date" length="" validate="" field="create_time" />
			<property name="createBy" type="Long" length="20" validate="" field="create_by" />
			<property name="updateTime" type="Date" length="" validate="" field="update_time" />
			<property name="updateBy" type="Long" length="20" validate="" field="update_by" />
			<property name="updateCount" type="Long" length="11" validate="" field="update_count" />
		</object>
	</model_layer>
</tsoftware>
