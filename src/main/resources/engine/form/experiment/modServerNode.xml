<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 节点服务器表 -->
		<object name="serverNode" table="t_server_node" primary="id"
			unique="code" order="id desc">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="code" type="String" length="20" field="code" />
			<property name="name" type="String" length="20" field="name" />
			<property name="ip" type="String" length="50" field="ip" />
			<property name="port" type="String" length="50" field="port" />
			<property name="accountNumber" type="String" length="50"
				field="account_number" />
			<property name="password" type="String" length="50"
				field="password" />
			<property name="emulator" type="String" length="200"
				field="emulator" />
			<property name="domainStoragePoolSrc" type="String"
				length="200" field="domain_storage_pool_src" />
			<property name="maxDomains" type="Long" length="20"
				initial="" field="max_domains" />
			<property name="maxMemory" type="Long" length="20"
				initial="" field="max_memory" />
			<property name="minDomains" type="Long" length="20"
				initial="" field="min_domains" />
		</object>
	</model_layer>
</tsoftware>
