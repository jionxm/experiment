<?xml version="1.0" encoding="UTF-8"?>
<tsoftware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 任务进度表 -->
		<object name="mission" table="t_mission" primary="id" unique=""
			order="">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="name" type="String" length="20" field="name" />
			<property name="decision" type="String" length="200" field="decision" />
			<property name="startTime" type="Date" length="" validate=""
				field="start_time" />
			<property name="endTime" type="Date" length="" validate=""
				field="end_time" />
			<property name="executor" type="Long" length="10" field="executor" />
			<property name="server" type="String" length="20" field="server" />
			<property name="teacherId" type="String" length="20" field="teacher_id" />
			<property name="db" type="String" length="20" field="db" />
			<property name="errorLog" type="String" length="500" field="error_log" />
			<property name="createTime" type="Date" length="" validate=""
				field="create_time" />
			<property name="createBy" type="Long" length="20" validate=""
				field="create_by" />
			<property name="updateTime" type="Date" length="" validate=""
				field="update_time" />
			<property name="updateBy" type="Long" length="20" validate=""
				field="update_by" />
			<property name="updateCount" type="Long" length="11"
				validate="" field="update_count" />

			<property name="executorName" type="String" length="20"
				field="" />
			<property name="createByName" type="String" length="20"
				validate="" field="" />
		</object>
	</model_layer>
</tsoftware>
