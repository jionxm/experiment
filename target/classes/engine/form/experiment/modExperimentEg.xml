<?xml version="1.0" encoding="UTF-8"?>
<tsoftware
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../componentConfig.xsd">
	<model_layer>
		<!-- 实验案例表 -->
		<object name="experimentEg" table="t_experiment_eg"
			primary="id" unique="" order="createTime desc">
			<property name="id" type="Long" length="10" initial=""
				field="id" />
			<property name="name" type="String" length="50" field="name" />
			<property name="serverNum" type="String" length="50" field="server_num" />
			<property name="demand" type="String" length="500"
				field="demand" />
			<property name="target" type="String" length="500"
				field="target" />
			<property name="content" type="String" length="500"
				field="content" />
			<property name="reportDemand" type="String" length="500"
				field="report_demand" />
			<property name="reportTarget" type="String" length="500"
				field="report_target" />
			<property name="reportContent" type="String" length="1000"
				field="report_content" />
			<property name="picId" type="Long" length="10"
				field="pic_id" />
			<property name="picName" type="String" length="50" field="" />
			<property name="createTime" type="Date" length=""
				validate="" field="create_time" />
			<property name="createBy" type="Long" length="20"
				validate="" field="create_by" />
			<property name="updateTime" type="Date" length=""
				validate="" field="update_time" />
			<property name="updateBy" type="Long" length="20"
				validate="" field="update_by" />
			<property name="updateCount" type="Long" length="11"
				validate="" field="update_count" />
			<property name="createByName" type="String" length="50"
				validate="" field="" />
			<property name="updateByName" type="String" length="50"
				validate="" field="" />
		</object>
	</model_layer>
</tsoftware>
